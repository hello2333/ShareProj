<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhning.shareproj">

    <application
        android:allowBackup="true"
        android:name=".utils.MyApp"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.CenterActivity"
            android:label="@string/title_activity_center" >
        </activity>
        <activity
            android:name=".activity.MineActivity"
            android:label="@string/title_activity_mine" >
        </activity>
        <activity
            android:name=".activity.CreateActivity"
            android:label="@string/title_activity_create" >
        </activity>
        <activity
            android:name=".activity.NoticeActivity"
            android:label="@string/title_activity_notice" >
        </activity>
        <activity
            android:name=".activity.DetailActivity"
            android:label="@string/title_activity_detail" >
        </activity>
    </application>

</manifest>